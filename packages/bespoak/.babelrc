{
  "presets": ["next/babel"],
  "plugins": [
    [
      "@babel/plugin-proposal-pipeline-operator",
      {
        "proposal": "minimal"
      }
    ],
    ["@babel/plugin-proposal-export-default-from"],
    [
      "extensible-destructuring",
      {
        "mode": "optout",
        "impl": "immutable"
      }
    ],
    [
      "module-resolver",
      {
        "root": ["./src"]
      }
    ]
  ]
}
